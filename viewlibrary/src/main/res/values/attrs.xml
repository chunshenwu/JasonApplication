<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircularIconView">
        <attr name="iconText" format="string" />
        <attr name="nameText" format="string" />
    </declare-styleable>

    <declare-styleable name="TabPoint">
        <attr name="pointWidth" format="dimension" />
        <attr name="pointHeight" format="dimension" />

        <attr name="pointColor" format="color" />
        <attr name="pointSelAlpha" format="float" />
        <attr name="pointUnSelAlpha" format="float" />

        <attr name="pointUseBackground" format="boolean" />

        <attr name="pointSelBackground" format="integer" />
        <attr name="pointUnSelBackground" format="integer" />
    </declare-styleable>

</resources>